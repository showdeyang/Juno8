YQWizard { background: #ffffff; }
YQMainWinDock { background: #ffffff; }

QFileDialog {
   background: #ffffff;
}

* {
   font-family:Microsoft YaHei;
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 25px;
    color: #4fa08b;
    background-color: #f5f5f5;
    margin-right:1px;
    border-radius:6px;
}
QDateEdit::down-arrow {
    border-image:url("static/xiala.png");
    height:10px;
    width: 12px;
}



/*xinjian*/
QPushButton#xinjian{
    border: 1px solid #4fa08b;
    border-radius: 6px;
    color:#ffffff;
    font-weight:bold;
    font-size:15px;
    outline: none;
    height:28px;
    letter-spacing:1px;
    background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 #96d0c1, stop:1 rgba(79,160,139,200));
}
QPushButton#xinjian:hover{
    background-color:#4fa08b;
    color:#ffffff;
    }

QToolButton, QCommandLinkButton{
    padding: 0 5px 0 5px;
    border-style: solid;
    border-width: 1px;
    border-radius: 6px;
    color: #5f5f5f;
    background-origin: content;
    background-position: right;
    background-repeat: no-repeat;
    background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 #fbfdfd, stop:0.5 #ffffff, stop:1 #fbfdfd);
    border-color:#4fa08b;
    height: 32px;
}

QToolButton:hover, QCommandLinkButton:hover{
    color: #ffffff;
    background-color: rgba(79,160,139,180);
}
QToolButton:pressed, QCommandLinkButton:pressed{
    color: #ffffff;
}
QToolButton:disabled, QCommandLinkButton:disabled{
    color: #616161;
    background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 #dce7eb, stop:0.5 #e0e8eb, stop:1 #dee7ec);
}
QToolButton::default, QCommandLinkButton::default{
    border: 2px solid transparent;
    color: #FFFFFF;
    background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 #84afe5, stop:1 #1168e4);
}



