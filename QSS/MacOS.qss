/* QComboBox gets the "on" state when the popup is open */
#combobox:!editable:on, #combobox::drop-down:editable:on {
  color: #4fa08b;
}
#combobox::drop-down { /* arrow part of the widget */
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 24px;
  border: none;
}

